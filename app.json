{
  "expo": {
    "name": "StoryApp",
    "slug": "StoryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package":"com.testuser.STORYREAD",
      "adaptiveIcon": {
        "foregroundImage": "./assets/s.jpg",
        "backgroundColor": "#FFFFFF"
      }
  
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
